{
  "name": "forge-version-checker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "wrangler deploy --dry-run --outdir dist",
    "clean": "rimraf .turbo .wrangler dist node_modules/.cache node_modules/.mf node_modules/.vitest",
    "dev": "wrangler dev --test-scheduled",
    "deploy": "wrangler deploy",
    "lint": "prettier . --check",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "bench": "vitest bench",
    "cf-typegen": "wrangler types"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.5.24",
    "@cloudflare/workers-types": "^4.20241022.0",
    "@egomaw/eslint-config": "^1.1.0",
    "@eslint/js": "^9.14.0",
    "@types/semver": "^7.5.8",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "lint-staged": "^15.2.10",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.2",
    "typescript-eslint": "^8.12.2",
    "wrangler": "^3.60.3"
  },
  "dependencies": {
    "fast-xml-parser": "^4.5.0",
    "itty-fetcher": "^0.9.4",
    "itty-router": "^5.0.18",
    "semver": "^7.6.3",
    "zod": "^3.23.8"
  }
}
