{
	"name": "forge-version-checker",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "wrangler deploy --dry-run --outdir dist",
		"clean": "rimraf .wrangler dist node_modules/.cache node_modules/.mf node_modules/.vitest",
		"dev": "wrangler dev --test-scheduled",
		"serve": "wrangler deploy",
		"lint": "prettier . --check",
		"test": "vitest",
		"cf-typegen": "wrangler types",
		"prepare": "husky"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.5.24",
		"@cloudflare/workers-types": "^4.20241022.0",
		"@egomaw/eslint-config": "^1.1.0",
		"@eslint/js": "^9.14.0",
		"cross-env": "^7.0.3",
		"husky": "^9.1.6",
		"eslint": "^9.14.0",
		"lint-staged": "^15.2.10",
		"rimraf": "^6.0.1",
		"typescript": "^5.5.2",
		"typescript-eslint": "^8.12.2",
		"vitest": "^2.1.4",
		"wrangler": "^3.60.3"
	},
	"dependencies": {
		"fast-xml-parser": "^4.5.0",
		"itty-fetcher": "^0.9.4",
		"itty-router": "^5.0.18",
		"version-compare": "^3.11.0",
		"version-range": "^4.14.0",
		"zod": "^3.23.8"
	}
}
